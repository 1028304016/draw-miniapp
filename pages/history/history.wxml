<view class="table">
 <view class="tr bg-w">
 <view class="th nameW">人员</view>
 <view class="th">值日内容</view>
 <view class="th">值日日期</view>
 </view>
 <!-- wx:key="{[code]}"-->
 <block wx:for="{{array}}" wx:for-item="item" wx:key="key">
	   <view class="tr bg-w">
	   <view class="th nameW">{{item.name}}</view>
	   <view class="th">{{item.content}}</view>
	   <view class="th">{{item.createTime}}</view>
	   </view>
 </block>
</view>